type MintResult = record {
    total_supply: nat64;
    user_balance: nat64;
};

service : {
    greet : (text) -> (text) query;
    get_total_supply : () -> (nat64) query;
    get_my_balance : () -> (nat64) query;
    mint_usdb : (nat64) -> (MintResult);
    burn_usdb : (nat64) -> (MintResult);
    transfer_usdb : (principal, nat64) -> ();
}
